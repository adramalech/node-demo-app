{
  "name": "node-app-demo",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "prepare": "husky",
    "lint": "eslint ./src  -c .eslintrc.cjs --ext '**/*.{json,md,yml,js,cjs,mjs,ts}' --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src -c .eslintrc.cjs --ext '**/*.{json,md,yml,js,cjs,mjs,ts}' --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier-format": "prettier --config .prettierrc.mjs \"**/*.js\" \"**/*.cjs\" \"**/*.mjs\" \"**/*.ts\" --write",
    "audit": "npm audit --registry='https://registry.npmjs.org/'",
    "typecheck": "tsc --noEmit",
    "test": "jest --watch"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.0",
    "@fastify/error": "^3.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "dotenv": "^16.4.1",
    "eslint-config-prettier": "^9.1.0",
    "fastify": "^4.26.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.3",
    "@types/prettier": "^2.1.1",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-yml": "^1.12.2",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.13.0",
    "husky": "^9.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "process": "^0.11.10",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
