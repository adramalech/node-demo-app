{
  "name": "node-app-demo",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit && eslint --config .eslintrc.js \"**/*.js\" \"**/*.ts\" --fix --quiet",
    "lint:verbose": "tsc && eslint --config .eslintrc.js \"**/*.js\" \"**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "tsc -w",
    "prettier-format": "prettier --config .prettierrc.js \"**/*.js\" \"**/*.ts\" --write",
    "build:dev": "npm run lint:verbose",
    "build:prod": "npm run lint",
    "start": "node ./dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,ts,md}": [
      "prettier --config .prettierrc.js \"**/*.js\" \"**/*.ts\" --write",
      "eslint --config .eslintrc.js \"**/*.js\" \"**/*.ts\" --fix"
    ]
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "base-64": "^1.0.0",
    "bunyan": "^1.8.15",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "fp-ts": "^2.11.5",
    "fp-ts-contrib": "^0.1.23",
    "immer": "^9.0.6",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.0",
    "io-ts-types": "^0.5.16",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-jwt": "^4.0.3",
    "koa-router": "^10.1.1",
    "monocle-ts": "^2.3.11",
    "newtype-ts": "^0.3.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/jest": "^27.0.3",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.4",
    "@types/koa-helmet": "^6.0.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.0.3",
    "@types/node": "^16.11.9",
    "@types/prettier": "^2.4.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  }
}
