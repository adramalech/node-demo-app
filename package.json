{
  "name": "node-app-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit && eslint --config .eslintrc.js \"**/*.js\" \"**/*.ts\" --fix --quiet",
    "lint:verbose": "tsc && eslint --config .eslintrc.js \"**/*.js\" \"**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "tsc -w",
    "prettier-format": "prettier --config .prettierrc.js \"**/*.js\" \"**/*.ts\" --write",
    "build:dev": "npm run lint:verbose",
    "build:prod": "npm run lint",
    "start": "node ./dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,ts,md}": [
      "prettier --config .prettierrc.js \"**/*.js\" \"**/*.ts\" --write",
      "eslint --config .eslintrc.js \"**/*.js\" \"**/*.ts\" --fix"
    ]
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "base-64": "^1.0.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "fp-ts": "^2.10.5",
    "fp-ts-contrib": "^0.1.23",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^1.2.2",
    "io-ts-types": "^0.5.16",
    "immer": "^9.0.6",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-jwt": "^4.0.1",
    "koa-router": "^10.0.0",
    "monocle-ts": "^2.3.9",
    "newtype-ts": "^0.3.4",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^6.0.2",
    "@types/koa-router": "^7.4.2",
    "@types/koa__cors": "^3.0.2",
    "@types/node": "^14.11.10",
    "@types/prettier": "^2.2.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": "14.x.x",
    "npm": "6.x.x"
  }
}
